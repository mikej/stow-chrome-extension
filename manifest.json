{
  "manifest_version": 3,
  "name": "Stow Chrome Extension",
  "version": "1.0.0",
  "description": "A Chrome extension for Stow",
  "action": {
    "default_popup": "index.html",
    "default_title": "Stow"
  },
  "permissions": ["storage", "tabs"],
  "commands": {
    "_execute_action": {
      "description": "Open the extension",
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "MacCtrl+Shift+S"
      }
    }
  }
}
